{
  "name": "Locker",
  "version": "1.1.0",
  "description": "Keep your links safe and secure with this Locker. No one can access your links without your password. ",
  "manifest_version": 4,
  "author": "Saurabh Khade",
  "action": {
    "default_popup": "index.html",
    "default_title": "Locker",
    "default_icon": {
      "16": "/images/maskable_icon.png",
      "32": "/images/maskable_icon.png",
      "48": "/images/maskable_icon.png",
      "128": "/images/maskable_icon.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage", "activeTab"],
  "icons": {
    "16": "/images/maskable_icon.png",
    "32": "/images/maskable_icon.png",
    "48": "/images/maskable_icon.png",
    "128": "/images/maskable_icon.png"
  }
}
